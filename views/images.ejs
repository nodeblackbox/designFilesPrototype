<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Image Gallery</title>
    <style>
        .gallery {
            display: flex;
            flex-wrap: wrap;
        }
        .gallery img {
            margin: 5px;
            width: 200px;
            height: auto;
            border: 1px solid #ccc;
        }
    </style>
</head>
<body>
    <h1>Image Gallery</h1>
    <div class="gallery">
        <% if (images && images.length > 0) { %>
            <% images.forEach(function(image) { %>
                <div>
                    <img src="<%= image.url %>" alt="<%= image.name %>">
                    <p><%= image.name %></p>
                </div>
            <% }); %>
        <% } else { %>
            <p>No images found.</p>
        <% } %>
    </div>
</body>
</html>